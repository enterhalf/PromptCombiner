{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Default capabilities for Prompt Combiner",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "core:path:default",
    "dialog:default",
    "dialog:allow-open",
    "fs:default",
    "fs:read-all",
    "fs:allow-read-file",
    "fs:allow-write-file",
    "fs:allow-create",
    "fs:allow-remove",
    "fs:allow-rename",
    "fs:allow-mkdir",
    "fs:allow-read-dir",
    "shell:default"
  ]
}
